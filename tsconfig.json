{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"lib": ["dom", "esnext"],
		"types": [
			"vite/client",
			"vitest/globals",
			"@cloudflare/workers-types",
			"@cloudflare/vitest-pool-workers"
		],
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@app-type": ["functions/api/[[route]].ts"],
			"@/*": ["./src/*"]
		}
	}
}
