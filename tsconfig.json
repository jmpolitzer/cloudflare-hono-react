{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"esModuleInterop": true,
		"strict": true,
		"skipLibCheck": true,
		"lib": ["dom", "esnext"],
		"types": ["@cloudflare/workers-types/2023-07-01"],
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"paths": {
			"@app-types": ["functions/api/[[route]].ts"],
			"@db/*": ["src/db/*"],
			"@resources/*": ["src/resources/*"]
		}
	}
}
